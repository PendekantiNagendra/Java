package com.hcl.ex;

class C1 {
	 static void test() {
		System.out.println("Welcome...");
	}
}

class C2 extends C1 {
	
	static void test() {
		System.out.println("Byee...");
	}
}

public class P28 {
	public static void main(String[] args) {
		C2 obj = new C2();
		C1 ob = new C1();
		obj.test();
		ob.test();
	}

}
