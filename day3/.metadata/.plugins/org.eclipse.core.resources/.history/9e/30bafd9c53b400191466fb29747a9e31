package com.hcl.abstrac;

/**
 * class Employ.
 *
 */

abstract class Emp {
  int empno;
  String name;
  double basic;
  /* 
 *method.
 */
@Override
  
  
  public String toString() {
    return "Employ [empno=" + empno + ", name=" + name + ", basic=" + basic + "]";
  }
  public Emp(int empno, String name, double basic) {
    super();
    this.empno = empno;
    this.name = name;
	this.basic = basic;
  }
}
