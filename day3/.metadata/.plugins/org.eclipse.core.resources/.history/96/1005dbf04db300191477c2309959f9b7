package com.hcl.Varars;

public class Attendance {

	public static void display(Object... ob) {
		// Object... is a Variable argument type

		for (Object object : ob) {
			System.out.println(object + " ");
		}
	}

	public static void show(String... name) {
		// String... is a Variable argument type

		for (String s : name) {
			System.out.print(s + " ");
		}
		System.out.println();
	}

	public static void main(String[] args) {
		show();
		show("Nagendra", "Rishab", "Sai");
		show("Yash", "Kumar");

		display();
		display(12,"Ram");
	}
}
