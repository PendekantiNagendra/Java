package com.hcl.cms;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="FbCount")
public class FbCount { 
	
	private int fbvalue;
    private int count;
    
    @Id
    @Column(name="fbvalue")
	public int getFbvalue() {
		return fbvalue;
	}
	public void setFbvalue(int fbvalue) {
		this.fbvalue = fbvalue;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}

}
